pipeline {
   agent any
   stages {
    stage ('PULL'){
        steps {
           git branch: 'main', credentialsId: 'github', url: 'git@github.com:teenabadki123/e-shopping-front.git'
        }
    }
    stage('Build') { 
        steps {
            sh '''
            curl -sL https://deb.nodesource.com/setup_12.x | sudo -E bash -
            sudo apt install nodejs
            node --version
            npm --version
            '''
            }
        }
   }
}
